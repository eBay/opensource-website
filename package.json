    "@ebay/ebayui-core": "^13.3.1",
    "@ebay/environment-ebay": "^1.2.0",
    "@ebay/fire-marshal-ebay": "^4.2.0",
    "@ebay/gatekeeper-ebay": "^4.3.1",
    "@ebay/global-header-ebay": "^6.7.35",
    "@ebay/global-header-marko-ebay": "^3.1.0",
    "@ebay/i18n-ebay": "^5.9.2",
    "@ebay/i18n-marko-ebay": "^1.1.1",
    "@ebay/locale-ebay": "^4.6.1",
    "@ebay/logging-inc": "^4.1.3",
    "@ebay/marko-run-adapter": "^0.1.15",
    "@ebay/metrics-ebay": "^4.3.2",
    "@ebay/module-config-inc": "^4.3.7",
    "@ebay/monitor-inc": "^5.5.0",
    "@ebay/oauth-ebay": "^4.6.1",
