    "stylelint-config-standard": "^36.0.0",
    "typescript": "^5.4.5",
    "vite": "^5.2.8",
    "vitest": "^1.4.0"
  },
  "gpaas": {
    "consumer-id": "urn:ebay-marketplace-consumerid:07908bef-d5d6-4994-a0db-e5da34b3654f",
