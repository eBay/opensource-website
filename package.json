{
  "name": "ebay-opensource",
  "description": "eBay Open Source: Hosting for eBay's Open Source projects, documentation, and partnerships",
  "version": "1.0.0",
  "type": "module",
  "repository": {
    "type": "git",
    "url": "https://github.com/eBay/opensource-website.git"
  },
  "dependencies": {
    "@evo-web/marko": "^0.1.0",
    "marko": "^6.0.0"
  },
  "devDependencies": {
    "@marko/run": "^0.8.0",
    "@marko/run-adapter-static": "^2.0.3",
    "gh-pages": "^6.3.0"
  },
  "private": true,
  "scripts": {
    "dev": "marko-run",
    "build": "marko-run build",
    "preview": "marko-run preview",
    "deploy": "BASE_URL=/opensource-website/ marko-run build && gh-pages --nojekyll -d dist/public"
  }
}
