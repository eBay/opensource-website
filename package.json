    "@ebay/skin": "^17.3.0",
    "@ebay/swu-ebay": "^3.4.0",
    "@ebay/tracking-ebay": "^4.14.3",
    "@ebay/tracking-marko-ebay": "^1.5.0",
    "@ebay/validate-internals-ebay": "^5.10.12",
    "@marko/express": "^2.1.0",
    "@marko/run": "^0.4.4",
    "express": "^4.19.2",
    "kraken-js": "^2.5.0",
    "marko": "^5.33.14"
  },
  "devDependencies": {
    "@commitlint/cli": "^19.2.1",
    "@commitlint/config-conventional": "^19.1.0",
    "@ebay/browserslist-config": "^2.8.0",
    "@ebay/telemetry-ebay": "^1.0.13",
    "@marko/compiler": "^5.35.11",
    "@marko/testing-library": "^6.2.0",
    "@marko/type-check": "^1.1.7",
