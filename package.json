{
  "name": "opensource-website",
  "version": "11.0.0",
  "private": true,
  "description": "eBay Open Source: Hosting for eBay's Open Source projects, documentation, and partnerships",
  "repository": {
    "type": "git",
    "url": "https://github.com/eBay/opensource-website.git"
  },
  "license": "UNLICENSED",
  "author": "jgonzalezdiaz",
  "type": "module",
  "main": "dist/index.cjs",
  "scripts": {
    "@ci:build": "yarn build",
    "@ci:lint": "yarn lint",
    "@ci:test": "yarn test run --coverage --no-color",
    "@deploy:prestart": "ebay-cdn-upload",
    "build": "rm -rf dist && marko-run build src",
    "clean": "rm -rf .cache .beans coverage dist node_modules/.{cache,vite,vitest}",
    "dev": "marko-run src",
    "format": "eslint --format unix --fix .; stylelint --formatter unix --fix '**/*.css'; prettier . --write --log-level=warn",
    "preinstall": "node --print '`{\"startTime\":${Date.now()}}`' > telemetry.json",
    "postinstall": "collect; husky",
    "lint": "mtc && eslint --format ./.eslint-format.cjs . && stylelint --custom-formatter ./.stylelint-format.js '**/*.css' && prettier . --check --log-level=warn",
    "preview": "marko-run preview src",
    "storybook": "storybook dev --host local.ebay.com --https --ssl-key node_modules/@ebay/marko-run-adapter/cert/server.key --ssl-cert node_modules/@ebay/marko-run-adapter/cert/server.crt",
    "swu:validate": "swu-validate ${PWD}",
    "test": "vitest"
  },
  "dependencies": {
    "@ebay/auth-ebay": "^5.6.1",
    "@ebay/brogan-ebay": "^5.0.3",
    "@ebay/cal": "^4.4.0",
