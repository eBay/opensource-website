{
  "brogan": {
    "showOnlyErrors": true
  },
  "raptor-config-ebay": {
    "globalconfig": true
  },
  "middleware": {
    "router": { "enabled": false },
    "static": { "enabled": false },
    "json": { "enabled": false },
    "multipart": { "enabled": false },
    "urlencoded": { "enabled": false },
    "cookieParser": { "enabled": false },
    "session": { "enabled": false },
    "favicon": { "enabled": false },
    "meta-router/match": { "enabled": false, "priority": 79 },
    "run-match": {
      "priority": 80,
      "module": "@ebay/marko-run-adapter/match"
    },
    "run-invoke": {
      "priority": 1000,
      "module": "@ebay/marko-run-adapter/invoke"
    },
    "device-detection": {
      "priority": 112,
      "module": {
        "name": "@ebay/device-detection-ebay",
        "method": "middleware"
      }
    },
    "i18n": {
      "priority": 114,
      "module": "@ebay/i18n-ebay/middleware"
    },
    "tracking": {
      "priority": 115,
      "module": "@ebay/tracking-ebay/middleware"
    },
    "pds": {
      "priority": 117,
      "module": "@ebay/pds-ebay/middleware"
    },
    "global-header": {
      "priority": 119,
      "module": "@ebay/global-header-ebay/middleware"
    },
    "site-speed": {
      "priority": 120,
      "module": "@ebay/site-speed-ebay"
    },
    "auth": {
      "priority": 121,
      "module": {
        "name": "@ebay/auth-ebay/middleware",
        "method": "auth"
      }
    }
  }
}
